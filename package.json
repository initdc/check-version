{
  "name": "check-version",
  "version": "0.0.0",
  "private": true,
  "description": "check version by webhook",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "test:graphql": "tsc --skipLibCheck && node ./lib/graph.js",
    "test:main": "tsc --skipLibCheck && node ./lib/main.js",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/initdc/check-version.git"
  },
  "keywords": [
    "check",
    "version",
    "webhook"
  ],
  "author": "initdc",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.1.0",
    "@actions/io": "^1.1.1",
    "got": "^11.8.2",
    "graphql": "^15.5.1",
    "lodash.at": "^4.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/lodash.at": "^4.6.6",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
